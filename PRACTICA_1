function Alumno(nombreA, edadA){
    this.nombre = nombreA
    this.edad = edadA
    }
    
for(let i=0; i<2;i++){
    var nombre = prompt("Escriba el nombre")
    var edad = prompt("Escribe la edad")
    alumnos.push(new Alumno(nombre, edad))
}
